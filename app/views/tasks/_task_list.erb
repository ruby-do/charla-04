<table class="table table-bordered">
  <thead>
    <th>
      Id
    </th>
    <th>
        Name
    </th>
    <th>
       Description
    </th>
  </thead>
  <tbody>
    <%tasks.each do |task| %>
    <tr>
      <td>
        <%= task.id%>
      </td>
      <td>
         <%= task.name%>
      </td>
      <td>
        <%= task.description%>
      </td>
    </tr>
    <%end %>
  </tbody>
</table>